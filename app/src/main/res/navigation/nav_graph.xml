        android:name="net.ankio.auto.ui.fragment.BookFragment"
        android:label="@string/title_books"
        tools:layout="@layout/fragment_book">

        <action
            android:id="@+id/action_bookFragment_to_bookEditFragment"
            app:destination="@id/bookEditFragment" />
    </fragment>

    <fragment
